on boot
    chmod 0660 /sys/kernel/oppo_display/notify_panel_blank
    chown system system /sys/kernel/oppo_display/notify_panel_blank

service fingerprint-inscreen-1-0 /system/bin/hw/lineage.biometrics.fingerprint.inscreen@1.0-service.RMX1931
    interface vendor.lineage.biometrics.fingerprint.inscreen@1.0::IFingerprintInscreen default
    class hal
    user system
    group system
    shutdown critical
